<!-- ^top ^top:attributes ^top[attributes] ^top{attributes} + -->
<!-- $top $top(attribute) $top.attribute $top:attribute + -->
<!-- $.top $.top() $.top[] + -->
<!-- .top .top[] + -->
<!-- ^if(-top ) ^if(-top ) -->

<!-- variable._top -->

	<dict>
		<key>Regex</key>
		<string>([\$\^]\w{1,512}\.?)(?&lt;top&gt;([\.\_][!\-\w]*))\z</string>
		<key>ShouldAppendCompletion</key>
		<false/>
		<key>ShouldCompleteAll</key>
		<false/>
		<key>ShouldCompleteInStringLiteral</key>
		<false/>
	</dict>

	<!-- $.top $.top(value) $.top{value} $.top[value]-->
		<dict>
			<key>Regex</key>
			<string>(?&lt;top&gt;(\$\.)[!\-\w]+[\(\[\{]?(\w*)?[\)\]\}]?)\z</string>
			<key>ShouldAppendCompletion</key>
			<false/>
			<key>ShouldCompleteAll</key>
			<false/>
			<key>ShouldCompleteInStringLiteral</key>
			<false/>
		</dict>

 	<!-- ^top ^top::attribute ^top:attribute -->

		<dict>
			<key>Regex</key>
			<!-- <string>(?&lt;top&gt;([\^\$]?)\w+)(?&lt;attribute&gt;(\.|:|::)?[!\-\w]*)?\z</string> -->
			<string>(?&lt;top&gt;\^[!\-\w]+)(?&lt;attribute&gt;(:|::)[!\-\w]*)?\z</string>
			<key>ShouldAppendCompletion</key>
			<false/>
			<key>ShouldCompleteAll</key>
			<false/>
			<key>ShouldCompleteInStringLiteral</key>
			<false/>
		</dict>

		<!-- $top $top:attribute $top.attribute -->

 		<dict>
			<key>Regex</key>
			<string>(?&lt;top&gt;\$[!\-\w]+)(?&lt;attribute&gt;(\.|:|\._)[!\-\w]*)?\z</string>
			<key>ShouldAppendCompletion</key>
			<false/>
			<key>ShouldCompleteAll</key>
			<false/>
			<key>ShouldCompleteInStringLiteral</key>
			<false/>
		</dict>

		<!-- .top $top ^top .top[]  $top(attribute) ^top{attribute} ^top[attribute]  -->

		<dict>
			<key>Regex</key>
			<string>(?&lt;top&gt;(\$|\^|\.)[!\-\w]+)[\[\(\{]?(?&lt;attribute&gt;[!\-\w]*)?[\]\)\}]?\z</string>
			<key>ShouldAppendCompletion</key>
			<false/>
			<key>ShouldCompleteAll</key>
			<false/>
			<key>ShouldCompleteInStringLiteral</key>
			<false/>
		</dict>

	

		<!-- ^if(-top ) ^if(-top ) -->

		<dict>
			<key>Regex</key>
			<string>\^if\(\s*(?&lt;top&gt;\-[!\-\w]+)(\s*\))?\z</string>
			<key>ShouldAppendCompletion</key>
			<false/>
			<key>ShouldCompleteAll</key>
			<false/>
			<key>ShouldCompleteInStringLiteral</key>
